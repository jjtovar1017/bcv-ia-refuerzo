{
  "name": "asistente-de-medios-bcv",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "deploy:setup": "chmod +x scripts/setup-deployment.sh && ./scripts/setup-deployment.sh",
    "deploy:web": "npm run build && vercel --prod",
    "deploy:web:preview": "npm run build && vercel",
    "deploy:android": "cd android && bundle exec fastlane build_apk",
    "deploy:android:release": "cd android && bundle exec fastlane release",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "supabase db seed",
    "capacitor:sync": "npx cap sync",
    "capacitor:run:android": "npx cap run android --live-reload --external",
    "capacitor:build:android": "npm run build && npx cap sync android && npx cap build android"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "^7.0.1",
    "@capacitor/core": "^7.4.2",
    "@capacitor/geolocation": "^7.1.4",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@google/genai": "^1.12.0",
    "@sentry/react": "^8.0.0",
    "@sentry/tracing": "^7.99.0",
    "axios": "^1.6.7",
    "jspdf": "^2.5.1",
    "leaflet": "^1.9.4",
    "mammoth": "^1.8.0",
    "pdfjs-dist": "^4.4.178",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.8.1",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.2",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.28.0",
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^22.14.0",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^1.3.1",
    "workbox-webpack-plugin": "^7.0.0"
  }
}
